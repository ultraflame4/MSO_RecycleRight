{
    "metadata": [
        {
            "src": [
                {
                    "src": ".",
                    "files": [
                        "**/*.csproj"
                    ]
                }
            ],
            "dest": "docfx/api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.{md,yml}"
                ],
                "src": "docfx/api",
                "dest": "api",
                "exclude": [
                    "docfx/_site/**"
                ]
            },
            {
                "files": [
                    "README.md",
                    "index.md",
                    "toc.yml"
                ],
                "src": ".",
                "dest": ""
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "output": "docfx/_site",
        "template": [
            "default",
            "modern"
        ],
        "globalMetadata": {
            "_appName": "MSO_RecycleRight Documentation",
            "_appTitle": "MSO_RecycleRight Documentation",
            "_enableSearch": true,
            "pdf": false
        }
    }
}